l: '',\n         pwd: ''\n      }\n   },\n\n   methods: {\n      login() {\n          var data = {\n            'email' : this.email,\n            'password': this.pwd,\n          }\n         axios\n         .post('http://localhost:9999/api/adminlogin', data\n        )\n         .then(response => {\n            console.log(response.data)\n            localStorage.setItem('id', response.data['id']);\n            localStorage.setItem('access_token', response.data['access_token']);\n            window.location.href=\"/home\"\n         })\n         .catch(error => {\n            console.log(error);\n         })\n      },\n\n\n      register(){\n         window.location.href='/register'\n      }\n   }\n}\n</script>\n<style>\nbody {\n    font-family: \"Lato\", sans-serif;\n}\n\n\n\n.main-head{\n    height: 150px;\n    background: #FFF;\n   \n}\n\n.sidenav {\n    height: 100%;\n    background-color: #000;\n    overflow-x: hidden;\n    padding-top: 20px;\n}\n\n\n.main {\n    padding: 0px 10px;\n}\n\n@media screen and (max-height: 450px) {\n    .sidenav {padding-top: 15px;}\n}\n\n@media screen and (max-width: 450px) {\n    .login-form{\n        margin-top: 10%;\n    }\n\n    .register-form{\n        margin-top: 10%;\n    }\n}\n\n@media screen and (min-width: 768px){\n    .main{\n        margin-left: 40%; \n    }\n\n    .sidenav{\n        width: 40%;\n        position: fixed;\n        z-index: 1;\n        top: 0;\n        left: 0;\n    }\n\n    .login-form{\n        margin-top: 80%;\n    }\n\n    .register-form{\n        margin-top: 20%;\n    }\n}\n\n\n.login-main-text{\n    margin-top: 20%;\n    padding: 60px;\n    color: #fff;\n}\n\n.login-main-text h2{\n    font-weight: 300;\n}\n\n.btn-black{\n    background-color: #000 !important;\n    color: #fff;\n}\n</style>\n"]}]}