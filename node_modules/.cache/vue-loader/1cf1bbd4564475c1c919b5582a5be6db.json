pe=\"text\" v-model=\"content\" id=\"input-2\" placeholder=\"Description\"> </b-form-textarea>\n        </b-col>\n       </b-row>\n\n         <b-button type=\"button\" variant=\"success\" @click=\"addPosts()\">Submit Post</b-button>\n     </b-container>\n         \n        </div>\n\n\n\n</template>\n\n<script>\nimport axios from \"axios\";\nexport default {\n\n  mounted() {\n    if(!localStorage.getItem('access_token')){\n      window.location.href='/';\n    }\n  },\n  data() {\n    return {\n      title: \"\",\n      content: \"\",\n      alerts:false,\n      mandatory:false,\n      message:null\n    };\n  },\n  methods: {\n    addPosts() {\n      if(this.title == \"\" || this.content == \"\"){\n           this.message = \"Both field are Mandatory\";\n           this.alerts = false;\n           this.mandatory = true;\n\n      }\n\n      const headers = {\n      'Accept': 'application/json',\n      Authorization : 'Bearer '+ localStorage.getItem('access_token')\n      }\n       const data = { \n            title: this.title,\n            content: this.content,\n      }\n        axios.post(\"http://localhost:9999/api/addpost\", data ,{ headers: headers })\n      .then(response => {\n        console.log(response.data);\n           this.title = \"\";\n           this.content = \"\";\n           this.message = \"Posted SuccessFully\";\n           this.mandatory = false;\n           this.alerts = true;\n           \n      })\n    }\n  }\n};\n</script>\n\n\n<style lang=\"css\">\n</style>"]}]}